{
  "settings": {
    "time_delta": 1.0,
    "window_size": 60,
    "horizon_size": 60,
    "session": {
      "total_runs_for_best_models": 10
    },
    "control_names": [
      "n",
      "deltal",
      "deltar"
    ],
    "state_names": [
      "u",
      "v",
      "p",
      "r",
      "phi",
      "Vw",
      "alpha_x",
      "alpha_y"
    ],
    "additional_tests": {},
    "target_metric": "nrmse",
    "metrics": {
      "rmse": {
        "metric_class": "deepsysid.pipeline.metrics.RootMeanSquaredErrorMetric",
        "parameters": {}
      },
      "nrmse": {
        "metric_class": "deepsysid.pipeline.metrics.NormalizedRootMeanSquaredErrorMetric",
        "parameters": {}
      }
    },
    "explanation_metrics": {
      "infidelity": {
        "metric_class": "deepsysid.explainability.metrics.NMSEInfidelityMetric",
        "parameters": {}
      },
      "lipschitz": {
        "metric_class": "deepsysid.explainability.metrics.LipschitzEstimateMetric",
        "parameters": {
          "n_disturbances": 50,
          "control_error_std": [
            3.09,
            0.00188,
            0.00175
          ],
          "state_error_std": [
            0.0167,
            0.00427,
            0.0000666,
            0.000119,
            0.000374,
            0.0103,
            0.00566,
            0.0075
          ]
        }
      },
      "simplicity": {
        "metric_class": "deepsysid.explainability.metrics.ExplanationComplexityMetric",
        "parameters": {
          "relevance_threshold": 0.1
        }
      }
    }
  },
  "models": [
    {
      "model_base_name": "QLag",
      "model_class": "deepsysid.models.linear.QuadraticControlLag",
      "static_parameters": {},
      "flexible_parameters": {
        "lag": [
          15,
          30,
          45,
          60
        ]
      }
    },
    {
      "model_base_name": "kLinReg",
      "model_class": "deepsysid.models.switching.klinreg.KLinearRegressionARXModel",
      "static_parameters": {
        "use_max_restarts": true,
        "zero_probability_restarts": 25
      },
      "flexible_parameters": {
        "lag": [
          15,
          30,
          45,
          60
        ],
        "n_modes": [
          2,
          4,
          6,
          8
        ]
      }
    },
    {
      "model_base_name": "LSTM+Init",
      "model_class": "deepsysid.models.recurrent.LSTMInitModel",
      "static_parameters": {
        "dropout": 0.25,
        "sequence_length": 50,
        "batch_size": 256,
        "learning_rate": 0.0025,
        "epochs_initializer": 600,
        "epochs_predictor": 1000,
        "loss": "mse"
      },
      "flexible_parameters": {
        "recurrent_dim": [
          64,
          128,
          192,
          256
        ],
        "num_recurrent_layers": [
          2,
          3,
          4
        ]
      }
    },
    {
      "model_base_name": "ReLiNet",
      "model_class": "deepsysid.models.switching.switchrnn.UnconstrainedSwitchingLSTMModel",
      "static_parameters": {
        "dropout": 0.25,
        "sequence_length": 50,
        "batch_size": 256,
        "learning_rate": 0.0025,
        "epochs_initializer": 600,
        "epochs_predictor": 1000,
        "loss": "mse"
      },
      "flexible_parameters": {
        "recurrent_dim": [
          64,
          128,
          192,
          256
        ],
        "num_recurrent_layers": [
          2,
          3,
          4
        ]
      }
    },
    {
      "model_base_name": "StableReLiNet",
      "model_class": "deepsysid.models.switching.switchrnn.StableSwitchingLSTMModel",
      "static_parameters": {
        "dropout": 0.25,
        "sequence_length": 50,
        "batch_size": 256,
        "learning_rate": 0.0025,
        "epochs_initializer": 600,
        "epochs_predictor": 1000,
        "loss": "mse"
      },
      "flexible_parameters": {
        "recurrent_dim": [
          64,
          128,
          192,
          256
        ],
        "num_recurrent_layers": [
          2,
          3,
          4
        ]
      }
    },
    {
      "model_base_name": "StableReLiNet-v2",
      "model_class": "deepsysid.models.switching.switchrnn.StableSwitchingLSTMModel",
      "static_parameters": {
        "dropout": 0.25,
        "sequence_length": 50,
        "batch_size": 256,
        "epochs_initializer": 800,
        "epochs_predictor": 1200,
        "loss": "mse"
      },
      "flexible_parameters": {
        "recurrent_dim": [
          32,
          64,
          128
        ],
        "num_recurrent_layers": [
          2,
          3,
          4
        ],
        "learning_rate": [
          0.001,
          0.0005,
          0.0001,
          0.00005
        ]
      }
    }
  ],
  "explainers": {
    "ReLiNet-Explainer": {
      "explainer_class": "deepsysid.explainability.explainers.switching.SwitchingLSTMExplainer",
      "parameters": {}
    },
    "LIME": {
      "explainer_class": "deepsysid.explainability.explainers.lime.LIMEExplainer",
      "parameters": {
        "num_samples": 100,
        "cv_folds": 5
      }
    }
  }
}